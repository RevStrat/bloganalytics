{
  "name": "RevStrat/bloganalytics",
  "description": "Pulls Google Analytics data and provides queued jobs for updating data",
  "homepage": "http://github.com/RevStrat/bloganalytics",
  "keywords": ["silverstripe", "blog", "analytics"],
  "license": "BSD-3-Clause",
  "authors": [
    {"name": "Rob Rankin","email": "rob@revolutionstrategy.com"}
  ],
  "support": {
    "issues": "http://github.com/RevStrat/bloganalytics/issues"
  },
  "require": {
    "silverstripe/cms": "^4",
    "silverstripe/framework": "^4",
    "silverstripe/blog": "^3.2",
    "symbiote/silverstripe-queuedjobs": "^4.2",
    "google/apiclient": "^2.0"
  },
  "autoload": {
    "psr-4": {
        "RevStrat\\BlogAnalytics\\": "src/"
    }
  },
  "extra": {
    "installer-name": "bloganalytics",
    "expose": [
        "client"
    ]
  }
}
